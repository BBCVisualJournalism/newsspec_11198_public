<div class="results-chart--wrapper">
    <h4>How this compares with other jobs:</h4>
    <div class="results-chart">
        <div class="results-chart--bg__h--wrapper">
            <div class="results-chart--bg__h">
                <div class="h-bar h-bar__1"></div>
                <div class="h-bar h-bar__2"></div>
                <div class="h-bar h-bar__3"></div>
                <div class="h-bar h-bar__4"></div>
            </div>
        </div>
        <div class="results-chart--bg__v">
            <div class="y-axis"></div>
            <div class="v-line"></div>
        </div>
        <div class="results-chart--labels">
            <span class="results-chart--label__y-axis results-chart--label--100">100%</span>
            <span class="results-chart--label__y-axis results-chart--label--75">75%</span>
            <span class="results-chart--label__y-axis results-chart--label--50">50%</span>
            <span class="results-chart--label__y-axis results-chart--label--25">25%</span>
            <span class="results-chart--label__y-axis results-chart--label--0">0%</span>
            <span class="results-chart--label--most">Most likely</span>
            <span class="results-chart--label--least">Least likely</span>
        </div>
        <div class="results-chart--chart">
            <img src="img/results-chart.png" />
        </div>
        <div class="results-chart--marker--wrapper">
            <div class="results-chart--marker"></div>
            <div class="results-chart--label"><strong id="results-chart--label__rank"></strong> of <span id="results-chart--label__total"></span></div>
        </div>
    </div>
    <div class="sharetools-holder"></div>
</div>
